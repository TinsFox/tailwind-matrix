{
  "name": "tailwind-design",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "prettier-plugin-tailwindcss": "^0.2.4",
    "eslint": "^8.35.0",
    "eslint-config-custom": "workspace:*",
    "eslint-plugin-react": "7.31.8",
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@7.13.5"
}